{
  "author": "Pedro Martin <pataruco@gmail.com>",
  "dependencies": {
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.1",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "swagger-ui-express": "^4.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/swagger-ui-express": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "yarn prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.tf": "bash -c 'terraform fmt -check'"
  },
  "name": "martin-blanco-api",
  "main": "dist/index.js",
  "repository": "git@github.com:pataruco/martin-blanco.git",
  "scripts": {
    "build": "bash ./scripts/compile.sh",
    "deploy:dev": "bash ./scripts/deploy-dev.sh",
    "deploy:prod": "bash ./scripts/deploy-prod.sh",
    "dev": "yarn ts-node ./src/index.ts",
    "importer": "yarn ts-node ./src/importer/index.ts"
  },
  "version": "1.0.0"
}
